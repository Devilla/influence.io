{
  "routes": [
    {
      "method": "GET",
      "path": "/integrations",
      "handler": "Integrations.find",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/integrations/:_id",
      "handler": "Integrations.findOne",
      "config": {
        "policies": []
      }
    },
    {
      "method": "POST",
      "path": "/connect/*",
      "handler": "Integrations.googleOauth",
      "config": {
        "policies": [],
        "prefix": ""
      }
    },
    {
      "method": "GET",
      "path": "/integrations/:provider/callback",
      "handler": "Integrations.callback",
      "config": {
        "policies": []
      }
    },

    {
      "method": "POST",
      "path": "/integrations",
      "handler": "Integrations.create",
      "config": {
        "policies": []
      }
    },
    {
      "method": "PUT",
      "path": "/integrations/:_id",
      "handler": "Integrations.update",
      "config": {
        "policies": []
      }
    },
    {
      "method": "DELETE",
      "path": "/integrations/:_id",
      "handler": "Integrations.destroy",
      "config": {
        "policies": []
      }
    }
  ]
}
