{
  "routes": [
    {
      "method": "GET",
      "path": "/payment/user",
      "handler": "Payment.findAllUserPayments",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/payment/plan/:id",
      "handler": "Payment.findAllPlanPayments",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/payment",
      "handler": "Payment.find",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/payment/:_id",
      "handler": "Payment.findOne",
      "config": {
        "policies": []
      }
    },
    {
      "method": "POST",
      "path": "/payment",
      "handler": "Payment.create",
      "config": {
        "policies": []
      }
    },
    {
      "method": "PUT",
      "path": "/payment/:_id",
      "handler": "Payment.update",
      "config": {
        "policies": []
