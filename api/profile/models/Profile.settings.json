{
  "connection": "default",
  "collectionName": "profile",
  "info": {
    "name": "profile",
    "description": "One user one profile concept."
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "user": {
      "model": "user",
      "via": "profile",
      "plugin": "users-permissions"
    },
    "isActive": {
      "type": "boolean",
      "multiple": false
    },
    "firstName": {
      "type": "string",
      "multiple": false
    },
    "lastName": {
      "type": "string",
      "multiple": false
    },
    "state": {
      "type": "string",
      "multiple": false
    },
    "address": {
      "type": "text",
      "multiple": false
    },
    "phoneNumber": {
      "type": "integer",
      "multiple": false
    },
    "companyName": {
      "type": "string",
      "multiple": false
    },
    "plan": {
      "type": "json",
      "multiple": false
    },
    "profile_payments": {
      "type": "json",
      "multiple": false
    },
    "uniqueVisitorQouta": {
      "type": "integer",
      "multiple": false
    },
    "uniqueVisitors": {
      "type": "integer",
      "multiple": false
    },
    "uniqueVisitorsQoutaLeft": {
      "type": "integer",
      "multiple": false
    }
  }
}
